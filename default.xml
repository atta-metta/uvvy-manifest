    <project name="libsss" path="sss" />
    <project name="travis-scripts" path="travis-ci" />
    <project name="libuia" path="uia" />

    <project name="dir_monitor" path="3rdparty/dir_monitor" />
    <project name="endian" path="3rdparty/endian" />
    <project name="metakit" path="3rdparty/metakit" />
    <project name="opus" path="3rdparty/opus" />
    <project name="rtaudio" path="3rdparty/rtaudio" />
    <project name="sodiumpp" path="3rdparty/sodiumpp" />

</manifest>
